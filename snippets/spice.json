{
    ".source.spice": {
        "import": {
            "prefix": "im",
            "body": "import \"${1:package}\" as ${2:identifier};",
            "description": "Snippet for an import statement"
        },
        "main function": {
            "prefix": "fmain",
            "body": "f<int> main() {\n\t$1\n}",
            "description": "Snippet for a main function"
        },
        "main function with cli args": {
            "prefix": "fmain",
            "body": "f<int> main(int argc, string[] argv) {\n\t$1\n}",
            "description": "Snippet for a main function with cli args"
        },
        "function def": {
            "prefix": "func",
            "body": "f<$1> ${2:functionName}($3) {\n\t$4\n}",
            "description": "Snippet for a function definition"
        },
        "precedure def": {
            "prefix": "proc",
            "body": "p ${1:procedureName}($2) {\n\t$3\n}",
            "description": "Snippet for a procedure definition"
        },
        "struct def": {
            "prefix": "struct",
            "body": "type ${1:StructName} struct {\n\t$2\n}",
            "description": "Snippet for a struct"
        },
        "interface def": {
            "prefix": "interface",
            "body": "type ${1:InterfaceName} interface {\n\t$2\n}",
            "description": "Snippet for an interface"
        },
        "generic type def": {
            "prefix": "gen",
            "body": "type ${1:TypeName} ${2:TypeAlts};",
            "description": "Snippet for a generic type definition"
        },
        "if": {
            "prefix": "if",
            "body": "if ${1:condition} {\n\t$0\n}",
            "description": "Snippet for an if statement"
        },
        "ifelse": {
            "prefix": "if",
            "body": "if ${1:condition} {\n\t$0\n} else {\n\t$1\n}",
            "description": "Snippet for an if else statement"
        },
        "for": {
            "prefix": "for",
            "body": "for ${1:int i = 0}; ${2:i < 10}; ${3:i++} {\n\t$4\n}",
            "description": "Snippet for a for loop"
        },
        "foreach": {
            "prefix": "foreach",
            "body": "foreach ${1:int item}: ${2:intArray} {\n\t$3\n}",
            "description": "Snippet for a foreach loop"
        },
        "foreach indexed": {
            "prefix": "foreach",
            "body": "foreach ${1:int i}, ${2:int item}: ${3:intArray} {\n\t$4\n}",
            "description": "Snippet for an indexed foreach loop"
        },
        "do": {
            "prefix": "do",
            "body": "do {\n\t$0\n} while ${1:condition};",
            "description": "Snippet for a do-while loop"
        },
        "while": {
            "prefix": "while",
            "body": "while ${1:condition} {\n\t$0\n}",
            "description": "Snippet for a while loop"
        },
        "single constant": {
            "prefix": "co",
            "body": "const ${1:type} ${2:name} = ${3:value}",
            "description": "Snippet for a constant"
        },
        "simple printf": {
            "prefix": "pf",
            "body": "printf(\"${1}\");",
            "description": "Snippet for a prinf statement without parameters"
        },
        "printf with params": {
            "prefix": "pff",
            "body": "printf(\"${1}\", ${2:var});",
            "description": "Snippet for a prinf statement with a parameter"
        },
        "sizeof": {
            "prefix": "so",
            "body": "sizeof(\"${1}\");",
            "description": "Snippet for a sizeof expression"
        },
        "alignof": {
            "prefix": "ao",
            "body": "alignof(\"${1}\");",
            "description": "Snippet for a alignof expression"
        },
        "unsafe": {
            "prefix": "us",
            "body": "unsafe {\n\t${1}\n}",
            "description": "Snippet for an unsafe block"
        },
        "assert": {
            "prefix": "as",
            "body": "assert ${1};",
            "description": "Snippet for an assert statement"
        },
        "hello world": {
            "prefix": "helloworld",
            "body": "f<int> main() {\n\tprintf(\"Hello World!\");\n}",
            "description": "Snippet for a hello world program"
        }
    }
}